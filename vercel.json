{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sin1"],
  "env": {
    "NEXTAUTH_URL": {
      "description": "The URL of your deployed application",
      "value": "@vercel-url"
    }
  },
  "build": {
    "env": {
      "NEXTAUTH_URL": "@vercel-url"
    }
  },
  "functions": {
    "src/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}